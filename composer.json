{
    "name":        "arcanedev/laravel-notes",
    "description": "Provides the ability to add notes to your Eloquent models in Laravel.",
    "keywords":    ["arcanedev", "laravel", "notes", "noteable"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelNotes",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               "^7.2.5",
        "arcanedev/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "mockery/mockery":     "^1.3.1",
        "phpunit/phpunit":     "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelNotes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelNotes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\LaravelNotes\\LaravelNotesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
